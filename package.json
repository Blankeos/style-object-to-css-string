{
  "name": "style-object-to-css-string",
  "version": "0.0.1",
  "description": "A micro-package to convert style objects to css strings.  Inspired by extensive usage of styled-components.",
  "main": "index.js",
  "scripts": {
    "test": "Node_Env=test jest",
    "start-dev": "nodemon --exec babel-node -- ./server/index.js",
    "test-dev": "Node_Env=test jest --watch",
    "prepush": "lint-staged",
    "lint": "eslint --ext .jsx --ext .js ."
  },
  "keywords": [
    "css",
    "styled-components"
  ],
  "author": "Tanner B. Hess Webber <tb@hesswebber.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-preset-airbnb": "^3.2.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-runner-eslint": "^0.7.3",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4",
    "regenerator-runtime": "^0.13.1"
  },
  "babel": {
    "presets": [
      "@babel/env",
      "airbnb"
    ],
    "ignore": [
      "node_modules",
      "compiled"
    ]
  },
  "jest": {
    "notify": true,
    "notifyMode": "always"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add",
      "jest --bail --findRelatedTests"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true,
      "browser": true,
      "es6": true
    },
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "vars": "all",
          "args": "after-used",
          "ignoreRestSiblings": false
        }
      ]
    },
    "parser": "babel-eslint"
  }
}
